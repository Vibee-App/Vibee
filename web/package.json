{
  "name": "web-view",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:front\"",
    "dev:front": "cd front && npm run dev",
    "dev:web": "rsbuild dev --open",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "@lynx-js/web-core": "^0.7.1",
    "@lynx-js/web-elements": "^0.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.2.14",
    "@rsbuild/plugin-react": "^1.1.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "concurrently": "^9.1.2",
    "typescript": "^5.8.2"
  }
}
